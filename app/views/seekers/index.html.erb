<p id="notice"><%= notice %></p>

<h1>Seekers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Seeker Signal</th>
      <th>Address</th>
      <th>City</th>
      <th>Zip</th>
      <th>Email</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seekers.each do |seeker| %>
      <tr>
        <td><%= seeker.first_name %></td>
        <td><%= seeker.last_name %></td>
        <td><%= seeker.seeker_signal %></td>
        <td><%= seeker.address %></td>
        <td><%= seeker.city %></td>
        <td><%= seeker.zip %></td>
        <td><%= seeker.email %></td>
        <td><%= seeker.phone %></td>
        <td><%= link_to 'Show', seeker %></td>
        <td><%= link_to 'Edit', edit_seeker_path(seeker) %></td>
        <td><%= link_to 'Destroy', seeker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seeker', new_seeker_path %>
